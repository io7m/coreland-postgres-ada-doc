(section
  (ref "pgada_database_clear")
  (title "PGAda.Database.Clear")
  (para function_desc "Free result storage.")
  (subsection
    (title "synopsis")
    (para-verbatim function_synopsis
"with PGAda.Database;

procedure Clear (Result : in out PGAda.Database.Result_T);
"))
  (subsection
    (title "description")
    (para
      "The " (item function "Clear") " function frees the storage associated
