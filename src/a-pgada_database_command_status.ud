(section
  (ref "pgada_database_command_status")
  (title "PGAda.Database.Command_Status")
  (para function_desc "Return command status from result.")
  (subsection
    (title "Synopsis")
    (para-verbatim function_synopsis
"with PGAda.Database;

function Command_Status (Result : in PGAda.Database.Result_T) return String;
"))
  (subsection
    (title "Description")
    (para
      "The " (item ada_function "Command_Status") " function returns the command
      status tag from the SQL command that generated the "
      (item parameter "Result") ".")))
